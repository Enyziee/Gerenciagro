{
	"name": "tcc-api",
	"version": "1.0.0",
	"description": "",
	"main": "src/app.ts",
	"scripts": {
		"test": "NODE_ENV=testing jest",
		"build": "tsc --build",
		"start": "tsc && node --require dotenv/config ./dist/app.js",
		"dev": "tsc-watch --onSuccess 'node --require dotenv/config dist/app.js'",
		"clear": "rm -rf dist/",
		"typeorm:generate": "typeorm-ts-node-commonjs migration:generate src/db/migrations/migration -d src/db/DataSource.ts",
		"typeorm:migrate": "typeorm-ts-node-commonjs migration:run -d src/db/DataSource.ts",
		"typeorm:revert": "typeorm-ts-node-commonjs migration:revert -d src/db/DataSource.ts",
		"typeorm:drop": "typeorm-ts-node-commonjs schema:drop -d src/db/DataSource.ts"
	},
	"keywords": [],
	"author": "Enyziee",
	"license": "ISC",
	"devDependencies": {
		"@tsconfig/node21": "^21.0.3",
		"@types/bcrypt": "^5.0.2",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/jsonwebtoken": "^9.0.6",
		"@types/morgan": "^1.9.9",
		"@types/pg": "^8.11.6",
		"@types/uuid": "^9.0.8",
		"@typescript-eslint/eslint-plugin": "^7.13.0",
		"@typescript-eslint/parser": "^7.13.0",
		"dotenv": "^16.4.5",
		"ts-node": "^10.9.2",
		"tsc-watch": "^6.2.0",
		"typescript": "^5.4.5"
	},
	"dependencies": {
		"bcrypt": "^5.1.1",
		"body-parser": "^1.20.2",
		"cors": "^2.8.5",
		"express": "^4.19.2",
		"express-validator": "^7.1.0",
		"jsonwebtoken": "^9.0.2",
		"morgan": "^1.10.0",
		"openmeteo": "^1.1.4",
		"pg": "^8.12.0",
		"reflect-metadata": "^0.2.2",
		"typeorm": "^0.3.20",
		"uuid": "^10.0.0"
	}
}
