	"main": "src/app.ts",
	"scripts": {
		"build": "tsc --build",
		"dev": "npx tsc && node --require dotenv/config ./dist/app.js",
		"typeorm:generate": "npx typeorm-ts-node-commonjs migration:generate src/db/migrations/migration -d src/db/DataSource.ts",
		"typeorm:migrate": "npx typeorm-ts-node-commonjs migration:run -d src/db/DataSource.ts",
		"typeorm:revert": "npx typeorm-ts-node-commonjs migration:revert -d src/db/DataSource.ts",
		"typeorm:drop": "npx typeorm-ts-node-commonjs schema:drop -d src/db/DataSource.ts"
	},
